                gameActionId: "INIT"
            }, callBack);
        },
        showLoadProgress: function(progress) {
            this.portal.setLoadProgress(progress);
        },
        maximizeGame: function() {
            "use strict";
            this.portal.maximizeClient();
        },
        minimizeGame: function() {
